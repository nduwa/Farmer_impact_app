{
  "expo": {
    "name": "react-native-farmer-impact",
    "slug": "react-native-farmer-impact",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash_rtc.png",
      "resizeMode": "contain",
      "backgroundColor": "#f8e4c0"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#fbf9ec"
      },
      "permissions": [
        "android.permission.DETECT_SCREEN_CAPTURE",
        "android.permission.CAMERA",
        "android.permission.CAMERA_ROLL",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.kwizera117.reactnativefarmerimpact"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you take pictures of files or documents when needed."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "0ee7a64b-714f-4b2e-a4ec-e6be93f50c86"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/0ee7a64b-714f-4b2e-a4ec-e6be93f50c86"
    }
  }
}
