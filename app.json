{
  "expo": {
    "name": "RTC Mobile",
    "slug": "rtc-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#fbf9ec"
    },
    "assetBundlePatterns": [
      "**/*",
      "!node_modules/**",
      "!**/*.md",
      "!**/README.md",
      "!**/.git/**",
      "!build/**",
      "!dist/**",
      "!**/*.config.js",
      "!tests/**",
      "!spec/**"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#fbf9ec"
      },
      "permissions": [
        "android.permission.DETECT_SCREEN_CAPTURE",
        "android.permission.CAMERA",
        "android.permission.CAMERA_ROLL",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.kwizera117.rtcmobile"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you take pictures of files or documents when needed."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "0bfd3209-f18b-4011-94e6-6e7309fa7e46"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/0bfd3209-f18b-4011-94e6-6e7309fa7e46"
    }
  }
}
